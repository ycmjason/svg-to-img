{
  "name": "@ycm.jason/svg-to-img",
  "version": "0.1.2",
  "description": "Just another svg-to-image library, that works in both browser and node.",
  "main": "lib/index.js",
  "bin": {
    "svg2img": "bin/svg2img"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "preversion": "npm run lint && npm run build && $EDITOR CHANGELOG.md",
    "version": "git add -A",
    "precommit": "lint-staged",
    "build": "npm run build:full && npm run build:core",
    "build:full": "rollup -c --environment FOR:full",
    "build:core": "rollup -c --environment FOR:core",
    "test": "npm run build && node test/index.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycmjason/svg-to-img.git"
  },
  "keywords": [
    "svg",
    "convert",
    "image",
    "png",
    "jpeg"
  ],
  "author": "Jason Yu <jason.yu@thehutgroup.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ycmjason/svg-to-img/issues"
  },
  "homepage": "https://github.com/ycmjason/svg-to-img#readme",
  "dependencies": {
    "@ycm.jason/logger": "^0.1.5",
    "puppeteer": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/polyfill": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@ycm.jason/eslint-config": "^1.3.1",
    "eslint": "^5.0.1",
    "express": "^4.16.3",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "opn": "^5.3.0",
    "prefetch-svg": "^0.1.1",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^4.0.0-beta.7",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-terser": "^1.0.1"
  }
}
