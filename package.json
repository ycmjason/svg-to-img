{
  "name": "@ycm.jason/svg-to-img",
  "version": "0.0.1",
  "description": "Just another svg-to-image library, that works.",
  "main": "lib/index.js",
  "bin": {
    "svg2img": "bin/svg2img"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "precommit": "lint-staged",
    "build": "rollup src/svg-to-img.js -o dist/svg-to-img.js -f umd --name svgToImg",
    "test": "node test/index.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycmjason/svg-to-img.git"
  },
  "keywords": [
    "svg",
    "convert",
    "image",
    "png",
    "jpeg"
  ],
  "author": "Jason Yu <jason.yu@thehutgroup.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ycmjason/svg-to-img/issues"
  },
  "homepage": "https://github.com/ycmjason/svg-to-img#readme",
  "dependencies": {
    "changedpi": "^1.0.1",
    "jimp": "^0.2.28",
    "puppeteer": "^1.5.0"
  },
  "devDependencies": {
    "@ycm.jason/eslint-config": "^1.3.1",
    "eslint": "^5.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "opn": "^5.3.0",
    "rollup": "^0.62.0"
  }
}
